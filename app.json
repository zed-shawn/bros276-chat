{
  "expo": {
    "name": "bros",
    "slug": "bros",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "2.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.bro.bros",
      "versionCode": 1,
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true
    },
    "updates": {
      "fallbackToCacheTimeout": 100
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
